<div class="page-header">
  <%= link_to servicio_tipos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    <%= t('back')%>
  <% end %>
  <%= link_to edit_servicio_tipo_path(@servicio_tipo), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    <%= t('edit')%>
  <% end %>
  <h1><%= t('show')%> <%= t('servicio_tipo', :default => 'servicio_tipo').humanize %></h1>
</div>

<dl class="dl-horizontal">
              <dt><%= t('tipo', :default => 'tipo').humanize %>:</dt>
        
        <dd><%= @servicio_tipo.tipo %></dd>   
     
                    <dt><%= t('descripcion', :default => 'descripcion').humanize %>:</dt>
        
        <dd><%= @servicio_tipo.descripcion %></dd>   
     
      </dl>
