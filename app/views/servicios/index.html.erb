<div class="page-header">
  <%= link_to new_servicio_path, class: 'btn btn-primary' do %>
  <span class="glyphicon glyphicon-plus"></span>
  <%= t('new')%> Servicio
  <% end %>
  <h1><%= t('listing')%> <%= t('servicio', :default => 'servicio').humanize.pluralize %></h1>
</div>

<div class="row">
  <div class="col-lg-6">
    <p>     
      <p><%= link_to "Importar Servicios de archivo CSV", new_servicio_import_path %></p>
    </p>
  </div>

  <div class="col-lg-6">
    <p>
    </p>
  </div>
</div><!--/row-->

<div class="table-responsive">
  <table id="servicios" 
  class="table table-striped table-bordered" 
  cellspacing="0" 
  width="100%", 
  data-source="<%= servicios_path(format: :json) %>">
  <thead>
    <tr>
      <th><%= t('nombre', :default => 'nombre').humanize %></th>
      <th><%= t('codigo', :default => 'codigo').humanize %></th>
      <th><%= t('precio', :default => 'precio').humanize %></th>
      <th><%= t('porcentaje_clinica', :default => 'porcentaje_clinica').humanize %></th>
      <th><%= t('porcentaje_doctor', :default => 'porcentaje_doctor').humanize %></th>
      <th><%= t('porcentaje_laboratorio', :default => 'porcentaje_laboratorio').humanize %></th>
      <th><%= t('servicio_tipo', :default => 'servicio_tipo').humanize %></th>
      <th></th>
      <th></th>
    </tr>
    </thead>
  <tbody>
  </tbody>
</table>
</div>
