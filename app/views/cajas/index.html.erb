<div class="page-header">
  <%= link_to new_caja_path, class: 'btn btn-primary' do %>
  <span class="glyphicon glyphicon-plus"></span>
  <%= t('new')%> Caja
  <% end %>
  <h1><%= t('listing')%> <%= t('caja', :default => 'caja').humanize.pluralize %></h1>
</div>

<div class="row">
  <div class="col-lg-6">
    <p>     
      <p><%= link_to "Importar Personal de Recepcion de archivo CSV", new_caja_import_path %></p>
    </p>
  </div>

  <div class="col-lg-6">
    <p>
    </p>
  </div>
</div><!--/row-->

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th><%= t('numero_cajero', :default => 'numero_cajero').humanize %></th>
        <th><%= t('persona', :default => 'persona').humanize %></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @cajas) do |caja| %>
      <td><%= caja.numero_cajero %></td> 
      <td><%= caja.persona.name %></td>
      <td><%= link_to t('show'), caja %></td>
      <td><%= link_to t('edit'), edit_caja_path(caja) %></td>
      <% end %>
    </tbody>
  </table>
</div>
