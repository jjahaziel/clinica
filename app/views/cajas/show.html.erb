<div class="page-header">
  <%= link_to cajas_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    <%= t('back')%>
  <% end %>
  <%= link_to edit_caja_path(@caja), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    <%= t('edit')%>
  <% end %>
  <h1><%= t('show')%> <%= t('caja', :default => 'caja').humanize %></h1>
</div>

<dl class="dl-horizontal">
              <dt><%= t('numero_cajero', :default => 'numero_cajero').humanize %>:</dt>
        
        <dd><%= @caja.numero_cajero %></dd>   
     
                    <dt><%= t('persona', :default => 'persona').humanize %>:</dt>
        
        <dd><%= @caja.persona.name %></dd>
     
      </dl>
